{
  "name": "nodejs-graphql-mongodb-starter-kit",
  "version": "1.0.0",
  "description": "NodeJS-GraphQL-MongoDB Starter Kit",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf ./build ./node_modules",
    "clear:cache": "rimraf ./node_modules/.cache",
    "lint": "yarn lint:eslint & yarn lint:prettier",
    "lint:eslint": "eslint --fix \"**/*.{j,t}s\"",
    "lint:prettier": "prettier -w \"**/*.{json,md,yaml}\"",
    "postinstall": "yarn clear:cache",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --inspect-brk --runInBand",
    "test:silent": "jest --silent --passWithNoTests",
    "test:watch": "jest --watch",
    "ts:build": "rimraf ./declarations/generated && tsc",
    "ts:check": "yarn tsc --noEmit",
    "ts:watch": "yarn ts:check --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oleksandrsydorenko/nodejs-graphql-mongodb-starter-kit.git"
  },
  "author": "Oleksandr Sydorenko (https://www.linkedin.com/in/oleksandrsydorenko)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oleksandrsydorenko/nodejs-graphql-mongodb-starter-kit/issues"
  },
  "homepage": "https://github.com/oleksandrsydorenko/nodejs-graphql-mongodb-starter-kit#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0",
    "mongoose": "^5.10.16",
    "nodemon": "^2.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt-cli": "^11.0.0",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  }
}
